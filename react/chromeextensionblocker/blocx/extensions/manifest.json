{
    "manifest_version": 3,
    "name": "Website Blocker",
    "version": "1.0",
    "description": "Blocks specific websites from being accessed",
    "permissions": [
      "tabs",
      "webNavigation",
      "storage"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    },
    "icons": {
      "128": "icon.png"
    }
  }
  